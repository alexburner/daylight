    "prettier": "1.5.3",
    "source-map-loader": "0.2.1",
    "tslint": "5.6.0",
    "tslint-config-prettier": "1.3.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-react": "3.2.0",
    "typescript": "2.3.2",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
