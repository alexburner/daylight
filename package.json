    "tslint": "tslint --fix 'src/**/*.+(ts|tsx|js|json)'",
    "style": "npm run prettier && npm run tslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexburner/daylight.website.git"
