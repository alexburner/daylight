  "license": "MIT",
  "dependencies": {
    "@types/moment": "2.13.0",
    "@types/react": "15.0.24",
    "@types/react-dom": "15.5.0",
    "@types/react-redux": "5.0.1",
    "@types/redux": "3.6.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "suncalc": "1.8.0"
